cpr_yz1_double: THEORY
BEGIN

    IMPORTING float@aerr754dp

modu(A, B: unb_double): unb_double = Dsub(A, Dmul(B, Dfloor(Ddiv(A,B))))

yz1 (Lat: unb_double): unb_double =
    Dfloor(Dadd(Dmul(Ddiv(modu(Lat, Ddiv(RtoD(360), Dsub(RtoD(60), RtoD(1)))), Ddiv(RtoD(360), Dsub(RtoD(60), RtoD(1)))), RtoD(131072)), RtoD(0.5)))

END cpr_yz1_double
