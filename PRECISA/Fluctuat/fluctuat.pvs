fluctuat:THEORY
BEGIN

IMPORTING float@aerr754sp

y(X: unb_single): unb_single = Fmul(Fsub(X, RtoS(1)), Fmul(Fsub(X, RtoS(1)), Fmul(Fsub(X, RtoS(1)), Fsub(X, RtoS(1)))))

z(X: unb_single): unb_single =
Fsub(Fmul(Fmul(Fmul(X, X),X),X),
     Fadd(Fmul(Fmul(Fmul(X, X),X),RtoS(4)),
          Fsub(Fmul(Fmul(X, X),RtoS(6)), Fadd(Fmul(X, RtoS(4)),RtoS(1)))))

t(X: unb_single): unb_single = Fsub(z(X), y(X))

END fluctuat
