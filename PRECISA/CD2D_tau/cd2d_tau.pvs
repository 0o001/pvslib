cd2d_tau: THEORY
BEGIN

IMPORTING float@aerr754sp

max (A,B: unb_single):unb_single = IF (A>B) THEN A ELSE B ENDIF

min (A,B: unb_single):unb_single = IF (A<B) THEN A ELSE B ENDIF

tau (Sx, Sy, Vx, Vy, T: unb_single): unb_single =  min (max (RtoS(0), Fneg(Fadd (Fmul (Sx, Vx), Fmul (Sy, Vy)))), Fmul (T, (Fadd (Fmul(Vx, Vx),Fmul(Vy, Vy)))))

END cd2d_tau
