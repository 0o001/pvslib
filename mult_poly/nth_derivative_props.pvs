nth_derivative_props[T: TYPE FROM real]  % [ parameters ]
		: THEORY

  BEGIN


 ASSUMING
 IMPORTING analysis@deriv_domain_def

   deriv_domain    : ASSUMPTION deriv_domain?[T]

ENDASSUMING

 noe: LEMMA
 not_one_element?[T]


 IMPORTING analysis@nth_derivatives[T]

 derivable_nderiv: LEMMA
 FORALL(f:[T -> real],n:nat):
 (derivable_n_times?(f,n)
 AND
 derivable?(nderiv(n,f)))
 IMPLIES
 derivable_n_times?(f,n+1)
 



  END nth_derivative_props
