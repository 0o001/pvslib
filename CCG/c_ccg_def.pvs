c_ccg_def[Val,Expr,Condition: TYPE,
    MT: TYPE, lt : (well_founded?[MT]), N: posnat]: THEORY
BEGIN

  IMPORTING cc_def[Val,Expr,Condition],
  	    digraphs@c_digraphs[CallingContext],
	    digraphs@c_digraphs_imp[CallingContext],
	    ccg_def_[Val,Expr,Condition,MT,lt,N,
		 digraph_type,digraph_verts,digraph_edges,
		 digraph_size,digraph_make]

END c_ccg_def
