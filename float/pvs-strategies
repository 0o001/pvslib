;; BEGIN interval-ia addendum
(defun add-bb-ia-op (ops)
  (loop for new-elem in ops
	do (when (not (member new-elem *ia-extended*)) (setq *ia-extended* (cons new-elem *ia-extended*)))))

(add-bb-ia-op '(("ulp_sp" "ULP_SP")
		("aebound_add""AEB_ADD")
		("aebound_sub""AEB_SUB")
		("aebound_mul""AEB_MUL")
		("aebound_div""AEB_DIV")
		("aebound_flr""AEB_FLR")
		("aebound_sqt""AEB_SQT")
		("aebound_neg""AEB_NEG")))
;; END interval-ia addendum

(defstep rewrite-aerr-expr ()
  (with-fresh-labels
   ((label 1))
   (with-tccs
    (then (expand "aebound_flr" label)
	  (expand "aebound_add" label)
	  (expand "aebound_mul" label)
	  (expand "aebound_div" label)
	  (expand "aebound_sub" label)
	  (expand "flr_aerr_bound" label)
	  (expand "add_aerr_bound" label)
	  (expand "mul_aerr_bound" label)
	  (expand "div_aerr_bound" label)
	  (expand "sub_aerr_bound" label)
	  (expand "flr_ulp_bound" label)
	  (expand "add_ulp_bound" label)
	  (expand "mul_ulp_bound" label)
	  (expand "div_ulp_bound" label)
	  (expand "sub_ulp_bound" label))))
  "" "")

 