aerr754sp_neg: THEORY
BEGIN

  IMPORTING ieee754sp_neg

  e1: VAR nonneg_real
  r1: VAR real
  
  aebound_neg(r1,e1): nonneg_real = e1

  Sneg_aerr: LEMMA
   FORALL(e1: nonneg_real, r1: real, f1: unb_single):
    abs(StoR(f1)-r1) <= e1 
    IMPLIES abs(StoR(Fneg(f1))- -r1) <= aebound_neg(r1,e1)

END aerr754sp_neg
