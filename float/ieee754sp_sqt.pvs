%
% IEEE 754 instantiation of the high level specification for single precision numbers.
%
ieee754sp_sqt: THEORY
BEGIN

  IMPORTING ieee754sp,
  	    cr_sqt[ieee754radix]

  f1 : VAR unb_single

  Fsqrt(f1|sqrt_pre(FtoR(f1)))
                      : { f: unb_single | closest_single?(sqrt(FtoR(f1)))(f) } =
    Fsqrt(single_precision_format)(f1)

  Fsqrt_correctly_rounded: LEMMA
   FORALL (f1|sqrt_pre(FtoR(f1))):
    Fsqrt(f1) = RtoS(sqrt(StoR(f1)))
        
END ieee754sp_sqt
