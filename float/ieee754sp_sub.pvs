%
% IEEE 754 instantiation of the high level specification for single precision numbers.
%
ieee754sp_sub: THEORY
BEGIN

  IMPORTING ieee754sp,
  	    cr_sub[ieee754radix]

  f1,f2 : VAR unb_single
  
  Fsub(f1,f2):{f:unb_single|closest_single?(FtoR(f1)-FtoR(f2))(f)} =
    Fsub(single_precision_format)(f1,f2)
    
  Fsub_correctly_rounded: LEMMA
    Fsub(f1,f2) = RtoS(StoR(f1) - StoR(f2))
    
END ieee754sp_sub
