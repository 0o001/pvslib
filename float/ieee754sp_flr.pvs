%
% IEEE 754 instantiation of the high level specification for single precision numbers.
%
ieee754sp_flr: THEORY
BEGIN

  IMPORTING ieee754sp,
  	    cr_flr[ieee754radix]

  f1 : VAR unb_single
  
  Ffloor(f1):{f:unb_single|closest_single?(floor(FtoR(f1)))(f)} =
    Ffloor(single_precision_format)(f1)

  Ffloor_correctly_rounded: LEMMA
    Ffloor(f1) = RtoS(floor(StoR(f1)))
    
END ieee754sp_flr
