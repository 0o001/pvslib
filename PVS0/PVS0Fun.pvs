PVS0Fun[T:TYPE+] : THEORY
BEGIN
  
  IMPORTING measure_termination[T,nat,LAMBDA(x,y:nat):x<y]

  PVS0Fun : TYPE = [[Val -> bool],[nat->[Val->Val]],[nat->[[Val,Val]->Val]],Def]

  terminating?(pvs0:PVS0Fun) : bool =
    terminates(pvs0`1,pvs0`2,pvs0`3)(pvs0`4)

  semantic_rel(pvs0:PVS0Fun,env:Environment,val:Val) : bool =
    semantic_rel(pvs0`1,pvs0`2,pvs0`3)(pvs0`4,env,val)

END PVS0Fun
